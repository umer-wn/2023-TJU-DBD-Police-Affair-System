{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  data() {\n    return {\n      policemenNumber: \"\",\n      policemenName: \"\",\n      policemenStatus: \"全部\",\n      policemenPosition: \"全部\",\n      policemenInfos: [],\n      selectPolicemenID: \"\",\n      stationAddress: \"\",\n      stationName: \"\",\n      stationID: \"\",\n      stationInfos: [],\n      selectStationID: \"\",\n      salary: 0,\n      err: \"警员不存在！\",\n      boxContent: \"\",\n      process: \"选择调度警员\",\n      failreason: \"\",\n      employInfo: []\n    };\n  },\n  methods: {\n    handleMouseMove(event) {\n      const x = event.pageX - event.target.offsetLeft;\n      const y = event.pageY - event.target.offsetTop;\n      event.target.style.setProperty('--x', `${x}px`);\n      event.target.style.setProperty('--y', `${y}px`);\n    },\n    async fetchpolicemenInfo() {\n      axios.get(\"http://localhost:7078/api/policemenInfo\", {\n        policemenNumber: this.policemenNumber,\n        policemenName: this.policemenName,\n        policemenStatus: this.policemenStatus,\n        policemenPosition: this.policemenPosition\n      }).then(res => {\n        this.policemenInfos = res.data;\n        for (var i = 0; i < this.policemenInfos.length; i++) {\n          if (this.policemenInfos[i].gender === \"F\") {\n            this.policemenInfos[i].gender = \"女\";\n          } else if (this.policemenInfos[i].gender === \"M\") {\n            this.policemenInfos[i].gender = \"男\";\n          }\n        }\n      }).catch(err => {\n        this.boxContent = this.err;\n        console.log(err);\n      });\n    },\n    async fetchStationInfo() {\n      try {\n        const response = await axios.get(`http://localhost:7078/api/stationInfo?stationID=${encodeURIComponent(this.stationID)}&stationName=${encodeURIComponent(this.stationName)}&stationAddress=${encodeURIComponent(this.stationAddress)}`);\n        this.stationInfos = response.data;\n      } catch (error) {\n        // 请求失败时的处理逻辑\n        ElMessage({\n          showClose: true,\n          message: \"获取警局列表失败！\",\n          type: 'error',\n          duration: 5000\n        });\n      }\n    },\n    async changeEmploy(index, policemenInfo) {\n      this.process = \"选择调动警局\";\n      this.selectPolicemenID = policemenInfo[\"pid\"];\n      console.log(this.selectPolicemenID);\n      this.err = \"警局不存在！\";\n    },\n    async goBack() {\n      this.process = \"选择调度警员\";\n      this.selectPolicemenID = \"\";\n      this.err = \"警员不存在！\";\n    },\n    async finish(index, StationInfo) {\n      this.process = \"确认信息\";\n      this.selectStationID = StationInfo[\"stationID\"];\n      console.log(this.selectStationID);\n      this.err = \"确认信息界面出错\";\n    },\n    async employ() {\n      console.log(this.salary);\n      if (this.salary <= 0) {\n        alert(\"雇佣薪资需大于〇！\");\n        return;\n      }\n      if (Number.isInteger(parseInt(salary))) {\n        alert(\"雇佣薪资需为整数！\");\n        return;\n      }\n      console.log(typeof this.salary);\n      axios.post(\"http://localhost:7078/api/addEmploy\", {\n        selectPolicemenID: this.selectPolicemenID,\n        selectStationID: this.selectStationID,\n        salary: this.salary\n      }).then(res => {\n        this.employInfo = res.data;\n        console.log(res.data);\n        if (res.data.failReason === \"success\") {\n          this.process = \"完成\";\n        } else {\n          this.process = \"失败\";\n        }\n      }).catch(err => {\n        this.boxContent = this.err;\n        console.log(err);\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","data","policemenNumber","policemenName","policemenStatus","policemenPosition","policemenInfos","selectPolicemenID","stationAddress","stationName","stationID","stationInfos","selectStationID","salary","err","boxContent","process","failreason","employInfo","methods","handleMouseMove","event","x","pageX","target","offsetLeft","y","pageY","offsetTop","style","setProperty","fetchpolicemenInfo","get","then","res","i","length","gender","catch","console","log","fetchStationInfo","response","encodeURIComponent","error","ElMessage","showClose","message","type","duration","changeEmploy","index","policemenInfo","goBack","finish","StationInfo","employ","alert","Number","isInteger","parseInt","post","failReason"],"sources":["C:\\GitHubWarehouse\\2023-TJU-DBD-Police-Affair-System\\My-front\\dbproject\\src\\components\\UrbanResidentmanagement\\RegionalDispatch.vue"],"sourcesContent":["<template>\r\n  <el-header class=\"sub-header\" @mousemove=\"handleMouseMove\">\r\n    <div>&nbsp;&nbsp;城区和居民管理&nbsp;>&nbsp;城市调度管理</div>\r\n  </el-header>\r\n    <main class=\"main\">\r\n      <section>\r\n          <!-- 这里开始输入 -->\r\n      <div v-if=\" process === '选择调度警员'\"> \r\n          <div class=\"inputcontainer\">\r\n          <label style=\"position: relative; display: block\">\r\n            <div class=\"ssqinputtext\">输入警员ID</div>\r\n            <input class=\"ssqinputinfobox\" type=\"text\" v-model=\"policemenNumber\" placeholder=\"警员ID\" />\r\n          </label>  \r\n          <label style=\"position: relative; display: block\">\r\n            <div class=\"ssqinputtext\">输入警员姓名</div>\r\n            <input class=\"ssqinputinfobox\" type=\"text\" v-model=\"policemenName\" placeholder=\"警员姓名\" />\r\n          </label>\r\n          </div>\r\n          \r\n          <div class=\"inputcontainer\">\r\n          <div class=\"selectcontainer\">\r\n            <div class=\"ssqinputtext\">选择工作状态</div>\r\n            <select class=\"zyhselect\" v-model=\"policemenStatus\">\r\n              <option selected value=\"全部\">全部</option>\r\n              <option value=\"在职\">在职</option>\r\n              <option value=\"离职\">离职</option>\r\n            </select>\r\n          </div>\r\n          <div class=\"selectcontainer\">\r\n            <div class=\"ssqinputtext\">选择职务</div>\r\n            <select class=\"zyhselect\" v-model=\"policemenPosition\">\r\n              <option selected value=\"全部\">全部</option>\r\n              <option value=\"学员\">学员</option>\r\n              <option value=\"警员\">警员</option>\r\n              <option value=\"警司\">警司</option>\r\n              <option value=\"警督\">警督</option>\r\n              <option value=\"警监\">警监</option>\r\n              <option value=\"总警监\">总警监</option>\r\n            </select>\r\n          </div>\r\n          </div>\r\n\r\n        <!-- 输入结束，下面是按钮 -->\r\n        <div class=\"btncontainer\">\r\n          <div class=\"leftbtn\">\r\n            <button class=\"ssqbutton1\" @click=\"fetchpolicemenInfo\" @mousemove=\"handleMouseMove\">\r\n              <span>查询</span>\r\n            </button>\r\n          </div>\r\n        </div>\r\n        <!-- 按钮结束，表格显示获取的警员信息 -->\r\n        <el-table v-if=\"policemenInfos.length > 0 \" :data=\"policemenInfos\" stripe height=\"450\"  @wheel.passive.stop>\r\n          <el-table-column prop=\"pid\" label=\"警号\" />\r\n          <el-table-column prop=\"name\" label=\"姓名\" />\r\n          <el-table-column prop=\"idn\" label=\"身份证号\" width=\"200px\" />\r\n          <el-table-column prop=\"birthday\" label=\"出生日期\" />\r\n          <el-table-column prop=\"gender\" label=\"性别\" />\r\n          <el-table-column prop=\"nation\" label=\"民族\" />\r\n          <el-table-column prop=\"phone\" label=\"联系电话\" />\r\n          <el-table-column prop=\"status\" label=\"状态\" />\r\n          <el-table-column prop=\"position\" label=\"职务\" />\r\n          <el-table-column align=\"right\" width=\"260px\">\r\n            <template #default=\"scope\">\r\n              <el-button size=\"small\" type=\"primary\" @click=\"changeEmploy(scope.$index, scope.row)\">进行人事调动</el-button>\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n        <!-- 错误提示 -->\r\n        <div v-else>{{ boxContent }}</div>\r\n      </div>\r\n\r\n\r\n      <div v-if=\"process === '选择调动警局'\" > \r\n        <div class=\"inputcontainer\">\r\n          <label style=\"position: relative; display: block\">\r\n            <div class=\"ssqinputtext\">警局ID</div>\r\n            <input class=\"ssqinputinfobox\" type=\"text\" v-model=\"stationID\" placeholder=\"输入警局ID\" />\r\n          </label>  \r\n          <label style=\"position: relative; display: block\">\r\n            <div class=\"ssqinputtext\">警局名称</div>\r\n            <input class=\"ssqinputinfobox\" type=\"text\" v-model=\"stationName\" placeholder=\"输入警局名称\" />\r\n          </label>          \r\n          <label style=\"position: relative; display: block\">\r\n            <div class=\"ssqinputtext\">警局地址</div>\r\n            <input class=\"ssqinputinfobox\" type=\"text\" v-model=\"stationAddress\" placeholder=\"输入警局地址\" />\r\n          </label>          \r\n          \r\n          </div>\r\n          \r\n\r\n        <!-- 输入结束，下面是按钮 -->\r\n        <div class=\"seaButton\">\r\n          <el-button type=\"primary\" @click=\"fetchStationInfo\">查询</el-button>\r\n        </div>\r\n\r\n\r\n      <el-table v-if=\"stationInfos.length >0\"  :data=\"stationInfos\" stripe height=\"450\"  @wheel.passive.stop>\r\n      <el-table-column prop=\"stationID\" label=\"警局编号\" sortable />\r\n      <el-table-column prop=\"stationName\" label=\"警局名称\" sortable />\r\n      <el-table-column prop=\"address\" label=\"所在地址\" sortable />\r\n      <el-table-column prop=\"budget\" label=\"预算\" sortable />\r\n      <el-table-column align=\"right\" width=\"260px\">\r\n\r\n        <template #header>\r\n          <table>\r\n            <tr>\r\n              <td><el-text type=\"primary\" style=\"display: inline-block; width: 170px\">当前警员警号：{{ selectPolicemenID }}</el-text> </td>\r\n              <td><el-button size=\"small\" type=\"primary\" @click=\"goBack\">重选警员</el-button></td>\r\n            </tr>\r\n          </table>\r\n        </template>\r\n        <template #default=\"scope\">\r\n              <el-button size=\"small\" type=\"primary\" @click=\"finish(scope.$index, scope.row)\">调入此局</el-button>\r\n            </template>\r\n      </el-table-column>\r\n       </el-table>       \r\n     <div v-else>{{ boxContent }}</div>\r\n      \r\n\r\n     </div>\r\n     \r\n     <div v-if=\"process === '确认信息'\" style=\"text-align: center\">\r\n      <!--直接展示当前雇佣信息并输入薪水-->>\r\n      <label style=\"position: relative; display: block\">\r\n        <div class=\"ssqinputtext\">当前调度信息：</div>\r\n        <div class=\"ssqinputtext\">调度警员警号：{{ selectPolicemenID }}</div> \r\n        <div class=\"ssqinputtext\">调入警局警号：{{ selectStationID }}</div>\r\n        <div class=\"ssqinputtext\">输入雇佣薪资</div>\r\n        <input class=\"ssqinputinfobox\" type=\"text\" v-model=\"salary\" placeholder=\"警员薪水\" />\r\n        <el-button type=\"primary\" @click=\"employ\">确认薪水</el-button>\r\n        <el-button type=\"primary\" @click=\"goBack\">返回</el-button>\r\n          </label>  \r\n      </div>\r\n\r\n\r\n     <div v-if=\"process === '完成'\" style=\"text-align: center\">\r\n    <el-result icon=\"success\" :title=\"'人事调动完成,任职开始时间为' + this.employInfo.employTime\"  sub-title=\"点击按钮返回警员查询列表\">\r\n      <template #extra>\r\n        <el-button type=\"primary\" @click=\"goBack\">返回</el-button>\r\n      </template>\r\n    </el-result>\r\n    </div>\r\n\r\n    <div v-if=\"process === '失败'\" style=\"text-align: center\">\r\n    <el-result icon=\"fail\" :title=\"'人事调动失败,薪资超过当局预算' + this.employInfo.failReason+'元'\"  sub-title=\"点击按钮返回警员查询列表\">\r\n      <template #extra>\r\n        <el-button type=\"primary\" @click=\"goBack\">返回</el-button>\r\n      </template>\r\n    </el-result>\r\n    </div>\r\n      </section>\r\n    </main>\r\n  </template>\r\n  \r\n  <script>\r\n  import axios from \"axios\";\r\n  \r\n  export default {\r\n    data() {\r\n      return {\r\n        policemenNumber: \"\",\r\n        policemenName: \"\",\r\n        policemenStatus: \"全部\",\r\n        policemenPosition: \"全部\",\r\n        policemenInfos: [],\r\n        selectPolicemenID: \"\",\r\n\r\n        stationAddress: \"\",\r\n        stationName: \"\",\r\n        stationID: \"\",\r\n        stationInfos: [],\r\n        selectStationID: \"\",\r\n        salary: 0,\r\n\r\n        err: \"警员不存在！\",\r\n        boxContent:\"\",\r\n        process: \"选择调度警员\",\r\n        failreason: \"\",\r\n\r\n        employInfo: [],\r\n      };\r\n    },\r\n    methods: {  \r\n      handleMouseMove(event) {\r\n          const x = event.pageX - event.target.offsetLeft;\r\n          const y = event.pageY - event.target.offsetTop;\r\n  \r\n          event.target.style.setProperty('--x', `${x}px`);\r\n          event.target.style.setProperty('--y', `${y}px`);\r\n        },\r\n      async fetchpolicemenInfo() {\r\n        axios.get(\"http://localhost:7078/api/policemenInfo\", {\r\n            policemenNumber: this.policemenNumber,\r\n            policemenName: this.policemenName,\r\n            policemenStatus: this.policemenStatus,\r\n            policemenPosition: this.policemenPosition,\r\n          })\r\n          .then((res) => {\r\n            this.policemenInfos = res.data;\r\n            for (var i = 0; i < this.policemenInfos.length; i++) {\r\n              if (this.policemenInfos[i].gender === \"F\") {\r\n                this.policemenInfos[i].gender = \"女\";\r\n              } else if (this.policemenInfos[i].gender === \"M\") {\r\n                this.policemenInfos[i].gender = \"男\";\r\n              }\r\n            }\r\n          })\r\n          .catch((err) => {\r\n            this.boxContent = this.err;\r\n            console.log(err);\r\n          });\r\n      },\r\n      async fetchStationInfo() {\r\n      try {\r\n        const response = await axios.get(`http://localhost:7078/api/stationInfo?stationID=${encodeURIComponent(this.stationID)}&stationName=${encodeURIComponent(this.stationName)}&stationAddress=${encodeURIComponent(this.stationAddress)}`);\r\n        this.stationInfos = response.data;\r\n       \r\n      } catch (error) {\r\n        // 请求失败时的处理逻辑\r\n        ElMessage({\r\n          showClose: true,\r\n          message: \"获取警局列表失败！\",\r\n          type: 'error',\r\n          duration: 5000,\r\n        });\r\n      }\r\n    },\r\n     async changeEmploy(index, policemenInfo) {\r\n      this.process = \"选择调动警局\";\r\n      this.selectPolicemenID = policemenInfo[\"pid\"];\r\n      console.log(this.selectPolicemenID);\r\n      this.err = \"警局不存在！\";\r\n    },\r\n     async goBack() {\r\n      this.process = \"选择调度警员\";\r\n      this.selectPolicemenID = \"\";\r\n      this.err = \"警员不存在！\";\r\n    },\r\n    async finish(index, StationInfo) {\r\n      this.process = \"确认信息\";\r\n      this.selectStationID = StationInfo[\"stationID\"];\r\n      console.log(this.selectStationID);\r\n      this.err = \"确认信息界面出错\";\r\n    },\r\n    async employ() {\r\n      console.log(this.salary);\r\n      if (this.salary<=0) {\r\n          alert(\"雇佣薪资需大于〇！\");\r\n          return;\r\n        }\r\n        if (Number.isInteger(parseInt(salary))) {\r\n          alert(\"雇佣薪资需为整数！\");\r\n          return;\r\n        }\r\n        \r\n          console.log(typeof this.salary);\r\n     \r\n        axios.post(\"http://localhost:7078/api/addEmploy\", {\r\n          selectPolicemenID: this.selectPolicemenID,\r\n          selectStationID: this.selectStationID,\r\n          salary: this.salary \r\n          })\r\n          .then((res) => {\r\n            this.employInfo=res.data;\r\n            console.log(res.data);\r\n            if (res.data.failReason === \"success\") {\r\n              this.process = \"完成\";\r\n            } else {\r\n              this.process = \"失败\";\r\n            }\r\n          })\r\n          .catch((err) => {\r\n            this.boxContent = this.err;\r\n            console.log(err);\r\n          });\r\n    },\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n   .inputcontainer{\r\n     display: flex;\r\n   }\r\n   .sub-header {\r\n      overflow: hidden;\r\n      display: flex;\r\n      position: absolute;\r\n      top: 70px;\r\n      left: 199px;\r\n      width: calc(100% - 199px);\r\n      he