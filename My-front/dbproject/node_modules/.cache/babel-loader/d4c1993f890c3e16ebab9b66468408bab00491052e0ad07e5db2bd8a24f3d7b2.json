{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"../../api/request\";\nexport default {\n  data() {\n    return {\n      pendingRequests: [],\n      // 将requests改为pendingRequests\n      approvedRequests: [],\n      rejectedRequests: [],\n      selectAll: false\n    };\n  },\n  mounted() {\n    this.fetchRequests();\n  },\n  methods: {\n    handleMouseMove(event) {\n      const x = event.pageX - event.target.offsetLeft;\n      const y = event.pageY - event.target.offsetTop;\n      event.target.style.setProperty(\"--x\", `${x}px`);\n      event.target.style.setProperty(\"--y\", `${y}px`);\n    },\n    toggleSelectAll() {\n      this.pendingRequests.forEach(request => {\n        request.selected = this.selectAll;\n      });\n    },\n    fetchRequests() {\n      axios.get(\"http://localhost:7078/api/manage\").then(response => {\n        const pendingRequests = []; // 存储待处理的申请\n        const approvedRequests = []; // 存储已同意的申请\n        const rejectedRequests = []; // 存储已拒绝的申请\n        response.data.forEach(request => {\n          if (request.status === \"待处理\") {\n            pendingRequests.push(request);\n          } else if (request.status === \"同意\") {\n            approvedRequests.push(request);\n          } else if (request.status === \"拒绝\") {\n            rejectedRequests.push(request);\n          }\n        });\n        this.pendingRequests = pendingRequests;\n        this.approvedRequests = approvedRequests;\n        this.rejectedRequests = rejectedRequests;\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n    approveRequest(request) {\n      axios.post(\"http://localhost:7078/api/manage\", {\n        s_number: request.s_number,\n        h_number: request.h_number,\n        F_level: request.f_level,\n        L_level: request.l_level,\n        reason: request.reason,\n        status: \"同意\"\n      }).then(response => {\n        this.fetchRequests();\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n    rejectRequest(request) {\n      axios.post(\"http://localhost:7078/api/manage\", {\n        s_number: request.s_number,\n        h_number: request.h_number,\n        F_level: request.f_level,\n        L_level: request.l_level,\n        reason: request.reason,\n        status: \"拒绝\"\n      }).then(response => {\n        this.fetchRequests();\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n    batchApprove() {\n      const selectedRequests = this.pendingRequests.filter(request => request.selected);\n      selectedRequests.forEach(request => {\n        this.approveRequest(request);\n      });\n    },\n    batchReject() {\n      const selectedRequests = this.pendingRequests.filter(request => request.selected);\n      selectedRequests.forEach(request => {\n        this.rejectRequest(request);\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","data","pendingRequests","approvedRequests","rejectedRequests","selectAll","mounted","fetchRequests","methods","handleMouseMove","event","x","pageX","target","offsetLeft","y","pageY","offsetTop","style","setProperty","toggleSelectAll","forEach","request","selected","get","then","response","status","push","catch","error","console","approveRequest","post","s_number","h_number","F_level","f_level","L_level","l_level","reason","rejectRequest","batchApprove","selectedRequests","filter","batchReject"],"sources":["C:\\GitHubWarehouse\\2023-TJU-DBD-Police-Affair-System\\My-front\\dbproject\\src\\components\\PoliceManagement\\PermissionManage.vue"],"sourcesContent":["<template>\r\n  <el-header class=\"sub-header\" @mousemove=\"handleMouseMove\">\r\n    <div>&nbsp;&nbsp;警员管理&nbsp;>&nbsp;修改申请管理</div>\r\n  </el-header>\r\n\r\n  <div class=\"container\" >\r\n    <div class=\"content\" @wheel.passive.stop>\r\n      <!-- 权限申请列表 -->\r\n      <div class=\"ssqtitletest\">\r\n        <span>权&nbsp;限&nbsp;申&nbsp;请&nbsp;列&nbsp;表</span>\r\n      </div>\r\n      <table class=\"custom-table\">\r\n        <thead>\r\n          <tr>\r\n            <th style=\"width: 20px\">\r\n              <input\r\n                type=\"checkbox\"\r\n                v-model=\"selectAll\"\r\n                @change=\"toggleSelectAll\"\r\n              />\r\n            </th>\r\n            <th style=\"width: 80px\">申请人</th>\r\n            <th style=\"width: 100px\">被修改人</th>\r\n            <th style=\"width: 100px\">原权限等级</th>\r\n            <th style=\"width: 120px\">修改权限等级</th>\r\n            <th style=\"width: 80px\">申请状态</th>\r\n            <th style=\"width: 200px\">申请原因</th>\r\n            <th style=\"width: 80px\">操作</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr\r\n            v-for=\"request in pendingRequests\"\r\n            :key=\"\r\n              request.h_number +\r\n              '-' +\r\n              request.s_number +\r\n              '-' +\r\n              request.F_level +\r\n              '-' +\r\n              request.L_level\r\n            \"\r\n          >\r\n            <td>\r\n              <input\r\n                type=\"checkbox\"\r\n                v-model=\"request.selected\"\r\n                v-if=\"request.status === '待处理'\"\r\n              />\r\n            </td>\r\n            <td>{{ request.h_number }}</td>\r\n            <td>{{ request.s_number }}</td>\r\n            <td>{{ request.f_level }}</td>\r\n            <td>{{ request.l_level }}</td>\r\n            <td>{{ request.status }}</td>\r\n            <td class=\"wrap-cell\">{{ request.reason }}</td>\r\n            <td>\r\n              <button\r\n                class=\"cpbutton\"\r\n                style=\"margin-top: -5px\"\r\n                @click=\"approveRequest(request)\"\r\n                v-if=\"request.status === '待处理'\"\r\n              >\r\n                <span>同意</span>\r\n              </button>\r\n              <button\r\n                class=\"cpbutton\"\r\n                @click=\"rejectRequest(request)\"\r\n                v-if=\"request.status === '待处理'\"\r\n              >\r\n                <span>拒绝</span>\r\n              </button>\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n      <!-- 批量审批操作区域 -->\r\n      <div>\r\n        <button class=\"cpbutton1\" @click=\"batchApprove\">\r\n          <span>批量同意</span>\r\n        </button>\r\n        <button\r\n          class=\"cpbutton1\"\r\n          style=\"margin-left: 40px\"\r\n          @click=\"batchReject\"\r\n        >\r\n          <span>批量拒绝</span>\r\n        </button>\r\n      </div>\r\n\r\n      <!-- 已同意的申请 -->\r\n      <div class=\"ssqtitletest\" style=\"margin-top: 80px\">\r\n        <span>已&nbsp;同&nbsp;意&nbsp;的&nbsp;申&nbsp;请</span>\r\n      </div>\r\n      <table class=\"custom-table\">\r\n        <!-- 表头 -->\r\n        <thead>\r\n          <tr>\r\n            <th>申请人警号</th>\r\n            <th>被修改人警号</th>\r\n            <th>原权限等级</th>\r\n            <th>修改权限等级</th>\r\n            <th>申请状态</th>\r\n            <th>申请原因</th>\r\n          </tr>\r\n        </thead>\r\n        <!-- 表格内容 -->\r\n        <tbody>\r\n          <tr\r\n            v-for=\"request in approvedRequests\"\r\n            :key=\"\r\n              request.h_number +\r\n              '-' +\r\n              request.s_number +\r\n              '-' +\r\n              request.F_level +\r\n              '-' +\r\n              request.L_level\r\n            \"\r\n          >\r\n            <td>{{ request.h_number }}</td>\r\n            <td>{{ request.s_number }}</td>\r\n            <td>{{ request.f_level }}</td>\r\n            <td>{{ request.l_level }}</td>\r\n            <td>{{ request.status }}</td>\r\n            <td>{{ request.reason }}</td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n\r\n      <!-- 已拒绝的申请 -->\r\n      <div class=\"ssqtitletest\" style=\"margin-top: 50px\">\r\n        <span>已&nbsp;拒&nbsp;绝&nbsp;的&nbsp;申&nbsp;请</span>\r\n      </div>\r\n      <table class=\"custom-table\">\r\n        <!-- 表头 -->\r\n        <thead>\r\n          <tr>\r\n            <th>申请人警号</th>\r\n            <th>被修改人警号</th>\r\n            <th>原权限等级</th>\r\n            <th>修改权限等级</th>\r\n            <th>申请状态</th>\r\n            <th>申请原因</th>\r\n          </tr>\r\n        </thead>\r\n        <!-- 表格内容 -->\r\n        <tbody>\r\n          <tr\r\n            v-for=\"request in rejectedRequests\"\r\n            :key=\"\r\n              request.h_number +\r\n              '-' +\r\n              request.s_number +\r\n              '-' +\r\n              request.F_level +\r\n              '-' +\r\n              request.L_level\r\n            \"\r\n          >\r\n            <td>{{ request.h_number }}</td>\r\n            <td>{{ request.s_number }}</td>\r\n            <td>{{ request.f_level }}</td>\r\n            <td>{{ request.l_level }}</td>\r\n            <td>{{ request.status }}</td>\r\n            <td>{{ request.reason }}</td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"../../api/request\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      pendingRequests: [], // 将requests改为pendingRequests\r\n      approvedRequests: [],\r\n      rejectedRequests: [],\r\n      selectAll: false,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.fetchRequests();\r\n  },\r\n  methods: {\r\n    handleMouseMove(event) {\r\n      const x = event.pageX - event.target.offsetLeft;\r\n      const y = event.pageY - event.target.offsetTop;\r\n\r\n      event.target.style.setProperty(\"--x\", `${x}px`);\r\n      event.target.style.setProperty(\"--y\", `${y}px`);\r\n    },\r\n    toggleSelectAll() {\r\n      this.pendingRequests.forEach((request) => {\r\n        request.selected = this.selectAll;\r\n      });\r\n    },\r\n    fetchRequests() {\r\n      axios\r\n        .get(\"http://localhost:7078/api/manage\")\r\n        .then((response) => {\r\n          const pendingRequests = []; // 存储待处理的申请\r\n          const approvedRequests = []; // 存储已同意的申请\r\n          const rejectedRequests = []; // 存储已拒绝的申请\r\n          response.data.forEach((request) => {\r\n            if (request.status === \"待处理\") {\r\n              pendingRequests.push(request);\r\n            } else if (request.status === \"同意\") {\r\n              approvedRequests.push(request);\r\n            } else if (request.status === \"拒绝\") {\r\n              rejectedRequests.push(request);\r\n            }\r\n          });\r\n          this.pendingRequests = pendingRequests;\r\n          this.approvedRequests = approvedRequests;\r\n          this.rejectedRequests = rejectedRequests;\r\n        })\r\n        .catch((error) => {\r\n          console.error(error);\r\n        });\r\n    },\r\n    approveRequest(request) {\r\n      axios\r\n        .post(\"http://localhost:7078/api/manage\", {\r\n          s_number: request.s_number,\r\n          h_number: request.h_number,\r\n          F_level: request.f_level,\r\n          L_level: request.l_level,\r\n          reason: request.reason,\r\n          status: \"同意\",\r\n        })\r\n        .then((response) => {\r\n          this.fetchRequests();\r\n        })\r\n        .catch((error) => {\r\n          console.error(error);\r\n        });\r\n    },\r\n    rejectRequest(request) {\r\n      axios\r\n        .post(\"http://localhost:7078/api/manage\", {\r\n          s_number: request.s_number,\r\n          h_number: request.h_number,\r\n          F_level: request.f_level,\r\n          L_level: request.l_level,\r\n          reason: request.reason,\r\n          status: \"拒绝\",\r\n        })\r\n        .then((response) => {\r\n          this.fetchRequests();\r\n        })\r\n        .catch((error) => {\r\n          console.error(error);\r\n        });\r\n    },\r\n    batchApprove() {\r\n      const selectedRequests = this.pendingRequests.filter(\r\n        (request) => request.selected\r\n      );\r\n      selectedRequests.forEach((request) => {\r\n        this.approveRequest(request);\r\n      });\r\n    },\r\n    batchReject() {\r\n      const selectedRequests = this.pendingRequests.filter(\r\n        (request) => request.selected\r\n      );\r\n      selectedRequests.forEach((request) => {\r\n        this.rejectRequest(request);\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.wrap-cell {\r\n  width: 40px;\r\n  white-space: pre-wrap;\r\n}\r\n.sub-header {\r\n  overflow: hidden;\r\n  display: flex;\r\n  position: absolute;\r\n  top: 70px;\r\n  left: 199px;\r\n  width: calc(100% - 199px);\r\n  height: 7vh;\r\n  min-height: 40px;\r\n  align-items: center; /* 文字竖直方向居中对齐 */\r\n  background-color: #1f2cdf;\r\n  box-shadow: inset -500px 0px 200px 0px rgba(4, 0, 113, 0.856);\r\n  color: #ffffff;\r\n  font-size: 28px;\r\n}\r\n.sub-header::before {\r\n  --size: 0;\r\n  content: \"\";\r\n  position: absolute;\r\n  left: var(--x);\r\n  top: var(--y);\r\n  width: var(--size);\r\n  height: var(--size);\r\n  background: radial-gradient(circle closest-side, #5a65ff, transparent);\r\n  transform: translate(-50%, -50%);\r\n  transition: width 0.2s ease, height 0.2s ease;\r\n}\r\n.sub-header:hover::before {\r\n  --size: 400px;\r\n}\r\n.container {\r\n  margin-top: 10vh;\r\n  min-width: 700px;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n  align-items: center;\r\n\r\n}\r\n.content {\r\n  width: 50%;\r\n  min-width: 1000px;\r\n  max-height:200vh;\r\n  overflow: auto;\r\n\r\n  box-shadow: 0px 0px 10px 2px rgba(123, 103, 75, 0.427);\r\n  background-color: rgba(255, 255, 255, 0.616);\r\n  margin-bottom: 30px;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: flex-start;\r\n  margin-top: 16px;\r\n  padding: 16px;\r\n\r\n  border-radius: 5px;\r\n  box-shadow: #9a9a9a 0px 0px 6px;\r\n  box-shadow: #777777 0px 0px 3px;\r\n  border-top: #0051ff 3px solid;\r\n  border-top: solid 3px transparent;\r\n}\r\n\r\n.ssqtitletest {\r\n  margin: 10px auto;\r\n  min-width: 800px;\r\n  height: 40px;